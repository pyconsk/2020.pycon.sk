#: pycon.py:8
msgid "PyCon SK 2020 | 27 - 29 March 2020 | Bratislava, Slovakia"
msgstr "PyCon SK 2020 | 27. - 29. marec 2020 | Bratislava"

#: templates/body.html:8
msgid "Official PyCon SK 2020 website"
msgstr "Oficiálna stránka PyCon SK 2020"

